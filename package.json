{
  "name": "asn-label-generator",
  "version": "1.0.0",
  "description": "CLI Tool for generating ASN labels with QR codes",
  "author": "Eiko Wagenknecht <git@eiko-wagenknecht.de>",
  "type": "module",
  "main": "dist/main.js",
  "bin": {
    "asn-label-generator": "./dist/main.js"
  },
  "packageManager": "pnpm@10.19.0",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=9.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "@tailwindcss/oxide",
      "core-js",
      "esbuild",
      "lefthook",
      "oxc-resolver"
    ]
  },
  "scripts": {
    "dev": "tsx src/cli/main.ts",
    "dev:web": "vite --config vite.config.web.ts",
    "build": "vite build",
    "build:web": "vite build --config vite.config.web.ts",
    "start": "node dist/main.js",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "lint": "tsc --noEmit && eslint --fix ./src && prettier --write ./src && biome check --fix --unsafe ./src",
    "lint:eslint": "eslint --fix ./src",
    "lint:prettier": "prettier --write ./src",
    "lint:biome": "biome check --fix --unsafe ./src",
    "format": "biome format . --write",
    "type-check": "tsc --noEmit",
    "check": "pnpm run lint && pnpm run type-check",
    "clean:npm": "pnpm exec rm -rf node_modules",
    "sync:npm": "pnpm i --frozen-lockfile",
    "upgrade:npm": "pnpm exec rm pnpm-lock.yaml && pnpm up --latest && pnpm i",
    "knip": "knip"
  },
  "dependencies": {
    "@hookform/resolvers": "5.2.2",
    "@radix-ui/react-checkbox": "1.3.3",
    "@radix-ui/react-collapsible": "1.1.12",
    "@radix-ui/react-label": "2.1.8",
    "@radix-ui/react-progress": "1.1.8",
    "@radix-ui/react-separator": "1.1.8",
    "@radix-ui/react-slot": "1.2.4",
    "@tailwindcss/vite": "4.1.18",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "commander": "14.0.2",
    "jspdf": "4.0.0",
    "lucide-react": "0.563.0",
    "qrcode": "1.5.4",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "7.71.1",
    "tailwind-merge": "3.4.0",
    "tailwindcss": "4.1.18",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.12",
    "@commitlint/cli": "20.3.1",
    "@commitlint/config-conventional": "20.3.1",
    "@commitlint/types": "20.3.1",
    "@eslint/js": "9.39.2",
    "@types/node": "24.10.9",
    "@types/qrcode": "1.5.6",
    "@types/react": "19.2.9",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.2",
    "@vitest/coverage-v8": "4.0.18",
    "eslint": "9.39.2",
    "globals": "17.1.0",
    "knip": "5.82.1",
    "lefthook": "2.0.15",
    "prettier": "3.8.1",
    "tsx": "4.21.0",
    "tw-animate-css": "1.4.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.53.1",
    "vite": "7.3.1",
    "vite-plugin-singlefile": "2.3.0",
    "vitest": "4.0.18"
  }
}
