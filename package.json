{
  "name": "asn-label-generator",
  "version": "1.0.0",
  "description": "CLI Tool for generating ASN labels with QR codes",
  "author": "Eiko Wagenknecht <git@eiko-wagenknecht.de>",
  "bin": {
    "asn-label-generator": "./dist/main.js"
  },
  "type": "module",
  "main": "dist/main.js",
  "scripts": {
    "dev": "tsx src/cli/main.ts",
    "dev:web": "vite --config vite.config.web.ts",
    "build": "vite build",
    "build:web": "vite build --config vite.config.web.ts",
    "start": "node dist/main.js",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "lint": "pnpm type-check && pnpm lint:oxlint && pnpm lint:oxfmt",
    "lint:oxlint": "oxlint --fix",
    "lint:oxfmt": "oxfmt ./src",
    "format": "oxfmt",
    "type-check": "tsc --noEmit",
    "clean:npm": "pnpm exec rm -rf node_modules",
    "sync:npm": "pnpm i --frozen-lockfile",
    "upgrade:npm": "pnpm exec rm pnpm-lock.yaml && pnpm up --latest && pnpm i",
    "knip": "knip"
  },
  "dependencies": {
    "@hookform/resolvers": "5.2.2",
    "@radix-ui/react-checkbox": "1.3.3",
    "@radix-ui/react-collapsible": "1.1.12",
    "@radix-ui/react-label": "2.1.8",
    "@radix-ui/react-progress": "1.1.8",
    "@radix-ui/react-separator": "1.1.8",
    "@radix-ui/react-slot": "1.2.4",
    "@tailwindcss/vite": "4.1.18",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "commander": "14.0.3",
    "jspdf": "4.1.0",
    "lucide-react": "0.564.0",
    "qrcode": "1.5.4",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-hook-form": "7.71.1",
    "tailwind-merge": "3.4.1",
    "tailwindcss": "4.1.18",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@commitlint/cli": "20.4.1",
    "@commitlint/config-conventional": "20.4.1",
    "@commitlint/types": "20.4.0",
    "@types/node": "24.10.13",
    "@types/qrcode": "1.5.6",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.4",
    "@vitest/coverage-v8": "4.0.18",
    "knip": "5.83.1",
    "lefthook": "2.1.1",
    "oxfmt": "0.32.0",
    "oxlint": "^1.47.0",
    "tsx": "4.21.0",
    "tw-animate-css": "1.4.0",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "vite-plugin-singlefile": "2.3.0",
    "vitest": "4.0.18"
  },
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.0.0"
  },
  "packageManager": "pnpm@10.29.2",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "core-js",
      "esbuild",
      "lefthook",
      "oxc-resolver"
    ]
  }
}
