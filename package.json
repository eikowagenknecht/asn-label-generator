{
  "name": "asn-label-generator",
  "private": true,
  "version": "1.0.0",
  "description": "CLI Tool for generating ASN labels with QR codes",
  "type": "module",
  "main": "dist/main.js",
  "bin": {
    "asn-label-generator": "./dist/main.js"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "dev": "vite-node src/main.ts",
    "build": "vite build",
    "start": "node dist/main.js",
    "lint": "tsc --noEmit && eslint --fix ./src && prettier --write ./src && biome check --write ./src",
    "lint:biome": "biome check --write ./src",
    "lint:eslint": "eslint --fix ./src",
    "lint:prettier": "prettier --write ./src",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "clean:npm": "pnpm exec rm -rf node_modules && pnpm exec rm pnpm-lock.yaml",
    "sync:npm": "pnpm i --frozen-lockfile",
    "upgrade:npm": "pnpm up --latest && pnpm i",
    "knip": "knip"
  },
  "dependencies": {
    "commander": "13.1.0",
    "pdfkit": "0.16.0",
    "qrcode": "1.5.4",
    "winston": "3.17.0",
    "zod": "3.24.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@commitlint/cli": "19.6.1",
    "@commitlint/config-conventional": "19.6.0",
    "@commitlint/types": "19.5.0",
    "@eslint/js": "9.18.0",
    "@types/node": "22.10.10",
    "@types/pdfkit": "0.13.8",
    "@types/qrcode": "1.5.5",
    "@vitejs/plugin-react": "4.3.4",
    "@vitest/coverage-v8": "3.0.4",
    "eslint": "9.18.0",
    "globals": "15.14.0",
    "knip": "5.43.3",
    "lefthook": "1.10.10",
    "prettier": "3.4.2",
    "rimraf": "6.0.1",
    "typescript": "5.7.3",
    "typescript-eslint": "8.21.0",
    "vite": "6.0.11",
    "vite-node": "3.0.4",
    "vitest": "3.0.4"
  }
}